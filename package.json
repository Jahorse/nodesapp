{
  "name": "nodesapp",
  "version": "0.1.0",
  "private": true,
  "license": "Attribution-NonCommercial-NoDerivatives 4.0 International",
  "main": "app.js",
  "scripts": {
    "build": "npm run clean && GENERATE_SOURCEMAP=false react-scripts build  && compress-cra",
    "dbuild": "npm run clean && react-scripts build  && compress-cra",
    "serve": "node app.js",
    "start": "react-scripts start",
    "clean": "rm -rf ./build/",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "ethers": "^5.6.6",
    "express-static-gzip": "^2.1.6",
    "firebase": "^9.8.1",
    "jquery": "^3.6.0",
    "memory-cache-ttl": "^1.0.52",
    "react": "^17.0.2",
    "react-cookie": "^4.1.1",
    "react-custom-events": "^1.0.6",
    "react-dom": "^17.0.2",
    "react-ga": "^3.3.0",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.2.2",
    "react-scripts": "^5.0.1",
    "reactstrap": "^9.0.2"
  },
  "devDependencies": {
    "compress-create-react-app": "^1.2.1",
    "eslint-plugin-react-hooks": "^4.5.0",
    "sass": "^1.52.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  }
}
